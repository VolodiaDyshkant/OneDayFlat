h2{
	font-family: 'Merriweather', serif;
	font-size: 40px;
	letter-spacing: 1px;
	max-width: 320px;
	width: 100%;
	position: relative;
	display: inline-block;
	color: #465457;
	padding: 20px 0;
	border-bottom: 2px solid rgba(0,0,0,.05);
	text-align: center;
	padding-bottom: 10px;
	text-shadow: 1px 1px ,
               -1px 1px #CCCCFF,
               -1px -1px #CCCCFF,
               1px -1px #CCCCFF;
}

h2:before {
  content: "";
  position: absolute;
  bottom: -2px;
  left: 50%;
  margin-left: -25%;
  width: 50%;
  height: 2px;
  background: #79F8D7;
}

h2:after {
  content: "";
  position: absolute;
  bottom: -6px;
  left: 50%;
  margin-left: -15%;
  width: 30%;
  height: 2px;
  background: #79F8D7;
}


h3 {
	position:center;

  display: inline-block;
  position: relative;
  letter-spacing: .05em;
  text-shadow: 1px 1px mediumturquoise,
               -1px 1px mediumturquoise,
               -1px -1px mediumturquoise,
               1px -1px mediumturquoise;
  color: white;
  transition: all 1s;
  }
 h3:before {
    content: "";
    position: absolute;
    top: -10px;
    right: -5px;
    bottom: -15px;
    left: 0;
    z-index: -1;
    background: linear-gradient(-45deg,
    rgba(72, 209, 204, 0) 2px, mediumturquoise 3px, rgba(72, 209, 204, 0) 3px ) repeat;
    background-size: 4px 4px;
    }
 h3:after {
    content: attr(data-name);
    position: absolute;
    top: 2px;
    left: 2px;
    z-index: -2;
    text-shadow: 1px 1px white,
                 2px 2px white,
                 3px 3px white,
                 4px 4px white;
    color: white;
    transition: all 1s;
    }

  h3:hover {
    color: #CCCC00;
    }
  h3:hover:before {
      animation: 5s move_lines infinite linear;
      }
  h3:hover:after {
      color: #CCCC00;
      text-shadow: 1px 1px #CCCC00,
        2px 2px #CCCC00,
        3px 3px #CCCC00,
        4px 4px #CCCC00;
      }
keyframes move_lines {
  100% {
    background-position: 40px 40px;
  }
}

form{
  margin:0 auto;
  max-width:95%;
  box-sizing:border-box;
  padding:40px;
